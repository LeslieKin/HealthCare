  <!-- ### Operation Step Contents :start -->
  <string name="operation_content_blood_pressure">1、挽起衣袖不分左右手
    \n2、套入检测布套包紧手臂
    \n3、点击右侧“启动检测”按钮
    \n4、按下设备的“血压”按钮开始检测
  </string>

  <string name="operation_content_blood_sugar">1、启动血糖仪
    \n2、将试纸放入血糖仪测量入口
    \n3、用针扎破手指，流出1滴血液
    \n4、手持血糖仪，让试纸吸取部分血液
    \n5、点击右侧“启动检测按钮”，等待测量完成</string>

  <string name="operation_content_blood_oxygen">1、启动测量仪
    \n2、将手指末端放在心率-血氧测量器内
    \n3、点击右侧“启动检测”按钮开始检测</string>

  <string name="operation_content_heart_rate">1、启动测量仪
    \n2、将手指末端放在心率-血氧测量器内
    \n3、点击右侧“启动检测”按钮开始检测</string>

  <string name="operation_content_temp">1、启动测量仪
    \n2、将体温测量器放在腋下位置
    \n3、测量仪显示测量温度
    \n4、点击右侧“启动检测按钮”，等待测量完成</string>

  <string name="operation_content_weight">暂无操作步骤</string>
